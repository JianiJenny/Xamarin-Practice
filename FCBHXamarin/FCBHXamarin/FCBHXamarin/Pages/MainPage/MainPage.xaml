<?xml version="1.0" encoding="utf-8" ?>
<rxui:ReactiveContentPage x:TypeArguments="mainPage:MainPageViewModel" xmlns="http://xamarin.com/schemas/2014/forms"
                          xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                          xmlns:rxui="clr-namespace:ReactiveUI.XamForms;assembly=ReactiveUI.XamForms"
                          xmlns:mainPage="clr-namespace:FCBHXamarin.Pages.MainPage;assembly=FCBHXamarin"
                          xmlns:resources="clr-namespace:FCBHXamarin.Resources;assembly=FCBHXamarin"
                          x:Class="FCBHXamarin.Pages.MainPage.MainPage"
                          NavigationPage.HasNavigationBar="False"
                          BackgroundColor="{StaticResource PrimaryBackground}">

    <StackLayout>
        <Label Text="{x:Static resources:AppResources.AppTitle}"
           Style="{StaticResource AppTitle}" />
    </StackLayout>

</rxui:ReactiveContentPage>
